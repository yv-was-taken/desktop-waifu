{
  "name": "desktop-waifu",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "trap 'kill $(jobs -p) 2>/dev/null' EXIT INT TERM; vite & sleep 2 && cargo run --manifest-path desktop-waifu-overlay/Cargo.toml",
    "dev:web": "vite",
    "build": "tsc && vite build && cargo build --release --manifest-path desktop-waifu-overlay/Cargo.toml",
    "build:web": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "@pixiv/three-vrm": "^3.4.5",
    "@pixiv/three-vrm-animation": "^3.4.5",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/postprocessing": "^3.0.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "ansi-to-html": "^0.7.2",
    "openai": "^6.16.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "^4.0.1",
    "three": "^0.182.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/cli": "^2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
